{
  "max_concurrent_downloads": 5,
  "default_download_path": "~/Downloads/zuup",
  "temp_directory": "~/.cache/zuup",
  "max_connections_per_download": 16,
  "user_agent": "Zuup/0.1.0 (Custom Download Manager)",
  "proxy_settings": {
    "enabled": false,
    "http_proxy": null,
    "https_proxy": null,
    "socks_proxy": null,
    "username": null,
    "password": null,
    "bypass_list": ["localhost", "127.0.0.1", "*.local"]
  },
  "logging_level": "INFO",
  "server_host": "127.0.0.1",
  "server_port": 8080,
  "theme": "dark",
  "auto_start_downloads": true,
  "show_notifications": true,
  "retry_attempts": 3,
  "timeout": 30,
  "bandwidth_limit": {
    "enabled": false,
    "download_speed_limit": null,
    "upload_speed_limit": null
  },
  "security": {
    "verify_ssl": true,
    "allow_insecure_connections": false,
    "certificate_path": null
  },
  "torrent_settings": {
    "enable_dht": true,
    "enable_lsd": true,
    "enable_upnp": true,
    "enable_natpmp": true,
    "listen_port_min": 6881,
    "listen_port_max": 6891,
    "max_upload_slots": 4,
    "seed_ratio_limit": 2.0,
    "seed_time_limit": 86400
  }
}